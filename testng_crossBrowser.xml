<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Cross_Browser"  parallel="tests" thread-count="2">
    <test name="chromeTest">
        <parameter name="BrowserName" value="chrome"/>
        <classes>
            <class name="pradeepfadatareacademy.Tests.MyStoreTests.IndexValidationText">
                <methods>
                    <include name="logoVAlidation"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="MyStoreTests">
        <parameter name="BrowserName" value="edge"/>
        <classes>
            <class name="pradeepfadatareacademy.Tests.MyStoreTests.IndexValidationText">
                <methods>
                    <include name="logoVAlidation"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>